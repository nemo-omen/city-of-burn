{
  "name": "@city-of-burn/server",
  "description": "",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {},
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 17.5.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "npm",
    "database": "sqlite",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ]
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "main": "src/",
  "browser": "dist/client",
  "scripts": {
    "dev": "nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "npm run compile && node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --require ts-node/register --recursive --extension .ts --exit",
    "test": "cross-env NODE_ENV=test npm run migrate && npm run mocha",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make"
  },
  "dependencies": {
    "@feathersjs/authentication": "^5.0.0-pre.27",
    "@feathersjs/authentication-local": "^5.0.0-pre.27",
    "@feathersjs/authentication-oauth": "^5.0.0-pre.27",
    "@feathersjs/configuration": "^5.0.0-pre.27",
    "@feathersjs/errors": "^5.0.0-pre.27",
    "@feathersjs/feathers": "^5.0.0-pre.27",
    "@feathersjs/knex": "^5.0.0-pre.27",
    "@feathersjs/koa": "^5.0.0-pre.27",
    "@feathersjs/schema": "^5.0.0-pre.27",
    "@feathersjs/socketio": "^5.0.0-pre.27",
    "@feathersjs/transport-commons": "^5.0.0-pre.27",
    "knex": "^2.2.0",
    "koa-static": "^5.0.0",
    "sqlite3": "^5.0.10",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/koa-static": "^4.0.2",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.0",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
